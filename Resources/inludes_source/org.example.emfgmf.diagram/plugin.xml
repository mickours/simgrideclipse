   <extension point="org.eclipse.ui.navigator.navigatorContent">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="org.example.emfgmf.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="org.example.emfgmf.topicmap.diagram.navigator.TopicMapNavigatorContentProvider" 
            labelProvider="org.example.emfgmf.topicmap.diagram.navigator.TopicMapNavigatorLabelProvider"
            icon="icons/obj16/TopicmapDiagramFile.gif"
            activeByDefault="false">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="topicmap"/>
               </and>
               <instanceof value="org.example.emfgmf.topicmap.diagram.navigator.TopicMapAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="org.example.emfgmf.topicmap.diagram.navigator.TopicMapAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="org.example.emfgmf.diagram.navigatorSorter" 
               class="org.example.emfgmf.topicmap.diagram.navigator.TopicMapNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="topicmap"/>
                  </and>
                  <instanceof value="org.example.emfgmf.topicmap.diagram.navigator.TopicMapAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="org.example.emfgmf.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="org.example.emfgmf.topicmap.diagram.navigator.TopicMapDomainNavigatorContentProvider" 
            labelProvider="org.example.emfgmf.topicmap.diagram.navigator.TopicMapDomainNavigatorLabelProvider"
            icon="icons/obj16/TopicmapDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="topicmap"/>
               </and>
               <and>
                  <instanceof value="org.eclipse.emf.ecore.EObject"/>
                  <test property="org.example.emfgmf.diagram.isDomainModelElement"/>
               </and>
            </or>
         </triggerPoints>
         <possibleChildren>
            <and>
       	       <instanceof value="org.eclipse.emf.ecore.EObject"/>
       	       <test property="org.example.emfgmf.diagram.isDomainModelElement"/>
       	    </and>
         </possibleChildren>
      </navigatorContent>
